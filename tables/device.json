{
  "name": "device",
  "comment": "设备",
  "joins": [
    {
      "table": "product",
      "local_field": "product_id",
      "foreign_field": "id",
      "field": "name",
      "as": "product"
    },
    {
      "table": "tenant",
      "local_field": "tenant_id",
      "foreign_field": "id",
      "field": "name",
      "as": "tenant"
    }
  ],
  "columns": [
    {
      "name": "id",
      "comment": "ID",
      "type": "string",
      "length": 50,
      "primary": true
    },
    {
      "name": "group_id",
      "comment": "组织ID",
      "type": "string",
      "length": 50,
      "indexed": true
    },
    {
      "name": "tenant_id",
      "comment": "租户ID",
      "type": "string",
      "length": 50,
      "indexed": true
    },
    {
      "name": "product_id",
      "comment": "产品ID",
      "type": "string",
      "length": 50,
      "indexed": true
    },
    {
      "name": "link_id",
      "comment": "连接ID",
      "type": "string",
      "length": 50,
      "indexed": true
    },
    {
      "name": "name",
      "comment": "名称",
      "type": "string",
      "length": 50
    },
    {
      "name": "description",
      "comment": "说明",
      "type": "text"
    },
    {
      "name": "online",
      "comment": "在线",
      "type": "bool",
      "default": "false"
    },
    {
      "name": "error",
      "comment": "错误",
      "type": "string",
      "length": 200
    },
    {
      "name": "longitude",
      "comment": "经度",
      "type": "int64"
    },
    {
      "name": "latitude",
      "comment": "纬度",
      "type": "int64"
    },
    {
      "name": "geo_code",
      "comment": "geo hash",
      "type": "string",
      "length": 50
    },
    {
      "name": "disabled",
      "comment": "禁用",
      "type": "bool",
      "default": "false"
    },
    {
      "name": "created",
      "comment": "创建时间",
      "type": "datetime",
      "created": true
    }
  ]
}