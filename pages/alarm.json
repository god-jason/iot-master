{
  "title": "报警日志",
  "template": "table",
  "toolbar": [
    {
      "key": "keyword",
      "type": "text",
      "placeholder": "请输入关键字"
    },
    {
      "type": "button",
      "icon": "search",
      "label": "搜索",
      "action": {
        "type": "script",
        "script": "this.keyword=this.toolbar.value.keyword; this.search()"
      }
    }
  ],
  "keywords": [
    "title",
    "message"
  ],
  "operators": [
    {
      "icon": "delete",
      "title": "删除",
      "confirm": "确认删除？",
      "action": {
        "type": "script",
        "script": "this.request.get('table/alarm/'+data.id+'/delete').subscribe(res=>{this.load()})"
      }
    }
  ],
  "columns": [
    {
      "key": "id",
      "label": "ID"
    },
    {
      "key": "device_id",
      "label": "设备ID",
      "action": {
        "type": "page",
        "page": "iot/device-detail",
        "params_func": "return {id: data.device_id}"
      }
    },
    {
      "key": "project_id",
      "label": "项目ID",
      "action": {
        "type": "page",
        "page": "iot/project-detail",
        "params_func": "return {id: data.project_id}"
      }
    },
    {
      "key": "title",
      "label": "标题"
    },
    {
      "key": "message",
      "label": "消息"
    },
    {
      "key": "level",
      "label": "等级"
    },
    {
      "key": "created",
      "label": "日期",
      "type": "date"
    }
  ],
  "search_api": "table/alarm/search",
  "mount": "if(this.params.device_id)this.filter.device_id=this.params.device_id;if(this.params.project_id)this.filter.project_id=this.params.project_id;"
}