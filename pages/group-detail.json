{
  "title": "组织详情",
  "template": "info",
  "toolbar": [
    {
      "icon": "edit",
      "type": "button",
      "label": "编辑",
      "action": {
        "type": "page",
        "page": "iot/group-edit",
        "params_func": "return {id: data.id}"
      }
    },
    {
      "icon": "delete",
      "type": "button",
      "label": "删除",
      "confirm": "确认删除？",
      "action": {
        "type": "script",
        "script": "this.request.get('table/group/delete/'+data.id).subscribe(res=>{this.navigate('/page/iot/group')})"
      }
    }
  ],
  "items": [
    {
      "key": "id",
      "label": "ID"
    },
    {
      "key": "user_id",
      "label": "管理员ID"
    },
    {
      "key": "name",
      "label": "名称"
    }
  ],
  "load_api": "table/group/detail/:id",
  "tabs": [
    {
      "title": "组织设备",
      "page": "iot/device",
      "params_func": "return {group_id: params.id}"
    },
    {
      "title": "成员管理",
      "page": "iot/member",
      "params_func": "return {group_id: params.id}"
    }
  ]
}